<?xml version="1.0" standalone="yes"?>

<custom-visualdsp-proc-xml
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:noNamespaceSchemaLocation="\Program Files\Analog Devices\VisualDSP 5.0\System\ArchDef\ADSP-custom-board.xsd"
     processor-family="BLACKFIN"
     file="ADSP-BF527-EZ-Kit-custom.xml">

<!-- ************************************************************************* -->
<!-- ******* ADSP-BF533-EZ-Kit-Version-legacy-custom.xml                       -->
<!-- ************************************************************************* -->
<!-- ******* Custom register reset definitions for Getting Started example.    -->
<!-- *******       <custom-register-reset-definitions>                         -->
<!-- ******* These are customizations to be made against the standard shipped  -->
<!-- ******* version in VisualDSP++ located in the installation directory:     -->
<!-- ******* install-dir\System\ArchDef\ADSP-BF533-proc.xml                    -->
<!-- ******* Copyright 2006-2007 Analog Devices, Inc.  All rights reserved.    -->
<!-- ************************************************************************* -->

<custom-register-reset-definitions>

     <!-- For BF533 EZ-KIT Lite's rev 1.6 and below use 0x13 -->
     <!-- For BF533 EZ-KIT Lite's rev 1.7 and above use 0x25 -->

     <register name="PLL_CTL" reset-value="0x3200" core="Common" />
     <register name="PLL_DIV" reset-value="0x0005" core="Common" />
     <register name="EBIU_SDRRC" reset-value="0x01A0" core="Common" />
     <register name="EBIU_SDBCTL" reset-value="0x13" core="Common" />
     <register name="EBIU_SDGCTL" reset-value="0x0091998d" core="Common" />

</custom-register-reset-definitions>

</custom-visualdsp-proc-xml> 

